{#
/**
 * @file
 * Theme override for a breadcrumb trail.
 *
 * Available variables:
 * - breadcrumb: Breadcrumb trail items.
 */
#}
{% if breadcrumb %}
  {% polymer import "mm-breadcrumb/mm-breadcrumb.html" %}
  <mm-breadcrumb breadcrumb="{{ breadcrumb | json_encode | e('html_attr') }}"
                aria-labelledby="system-breadcrumb"
                role="navigation"
                class="breadcrumb"
                heading="{{'Breadcrumb'|t| e('html_attr')}}">
  </mm-breadcrumb>
{% endif %}
