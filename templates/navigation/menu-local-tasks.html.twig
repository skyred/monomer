{#
/**
* @file
* Theme override to display primary and secondary local tasks.
*
* Available variables:
* - primary: HTML list items representing primary tasks.
* - secondary: HTML list items representing primary tasks.
*
* Each item in these variables (primary and secondary) can be individually
* themed in menu-local-task.html.twig.
*/
#}
{% polymer import "mm-tabs/mm-tabs.html" %}
{% if primary %}
  <mm-tabs heading="{{ 'Primary tabs'|t }}"
           wrapper-class="tabs--primary"
           tabs="{{ primary | json_encode | e('html_attr') }}"></mm-tabs>
{% endif %}
{% if secondary %}
  <mm-tabs heading="{{ 'Secondary tabs'|t }}"
           wrapper-class="tabs--secondary"
           tabs="{{ secondary | json_encode | e('html_attr') }}"></mm-tabs>
{% endif %}
